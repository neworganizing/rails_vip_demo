<table>
<tr><td><b>name</b></td><td><%=h @state.name %></td></tr>
<tr><td><b>election administration</b></td><td><%= @state.election_administration.nil? ? '(none)' : link_to(@state.election_administration.name ? @state.election_administration.name : "Election Administration", @state.election_administration) %></td></tr>
</table>
<table>
<b>Localities</b>
<tr>
<% @state.localities.each do |l| %>
  <td>
            <%= link_to l.name, l %>
  </td>
  <%= cycle("","","","</tr><tr>") %>
<% end %>
</tr>
</table>
