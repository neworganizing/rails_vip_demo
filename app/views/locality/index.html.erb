<table>
<tr><th>state</th><th>name</th><th>type</th><th></th></tr>
<% @localities.each do |l| %>
<tr style="background-color: <%= cycle("#DDDDDD","none")%>; ">
  <td><%=h l.state.name %></td>
  <td><%= link_to l.name, l %></td>
  <td><%= l.type %></td>
  <td><%= link_to "precincts", :controller => "precinct", :locality => l.id %></td>
</tr>
<% end %>
</table>
